<div class="panel panel-default">
  <div class="panel-heading">
    <h3>Users</h3>
    <div class="btn btn-primary" href="/administration/new"> Create User
  </div>
  </div>
  <table class="table" border="1">
   <tr>
   	 <th> First Name </th>
   	 <th> Last Name </th>
   	 <th> Email </th>
   </tr>
   <% @users.each do |user| %>
     <tr>
     	<td><%=user.first_name%> </td>
     	<td><%=user.last_name%> </td>
     	<td><%=user.email%> </td>
     </tr>	
    <% end %>
  </table>
</div>
